          <div class="user-details">
            <label for="user-email">Email:</label>
            <span id="user-email">johndoe@example.com</span>
          </div>
          <div class="user-details">
            <label for="user-phone">Phone:</label>
            <span id="user-phone">+1 (123) 456-7890</span>
          </div>
          <div class="user-details">
            <label for="user-address">Employee ID:</label>
            <span id="user-address">123AD</span>
          </div>
          <div class="user-details">
            <label for="user-address">Date of Birth:</label>
            <span id="user-address">1/09/1997</span>
          </div>
        </div>
    </div>
