<form action="/display" method="post"></form>
<div class="tb">
      <table class="table table-bordered" id="childTable">
            <thead class="table-info">
                  <tr>
                        <!-- <th scope="col">Sl. No.</th> -->

                  <tr>
                        <th scope="col">Date</th>
                        <th scope="col">Destination</th>
                        <th scope="col">price</th>
                        <th scope="col">upi</th>
                        <th scope="col">Name</th>
                  </tr>

            </thead>

            <tbody style="width:25;">
                  {{#each data}}
                  <tr>
                        <!-- <th scope="row">1</th> -->
                        <td class="col-1">


                              <input type="text" name="date of journey" value={{this.dt}} class="form-control"
                                    readonly />

                        </td>
                        <td class="col-sm-4" id="cl">

                              <input type="text" value={{this.desti}} name="destination" class="form-control"
                                    readonly />
                        </td>
                        <td class="col-1">

                              <input type="text" value={{this.price}} name="price" class="form-control" readonly />
                        </td>
                        <td class="col-2">

                              <input type="text" value={{this.upi}} name="upi" class="form-control" readonly />
                        </td>
                        <td class="col-sm-4">

                              <input type="text" value={{this.name}} name="enter-name" class="form-control" readonly />
                  </tr>
                  {{/each}}
            </tbody>
      </table>
      <div class="update-status">
            {{!-- <input type="button" class="btn btn-block btn-default" id="addrow" onclick="childrenRow()"
                  value="Update" /> --}}
      </div>
</div>
